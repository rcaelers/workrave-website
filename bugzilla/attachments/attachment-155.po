#: ../backend/src/DistributionSocketLink.cc:2061
#: ../backend/src/DistributionSocketLink.cc:2135
#, c-format
msgid "Client %s:%d closed connection."
msgstr "Clientul %s:%d a închis conexiunea."

#: ../backend/src/DistributionSocketLink.cc:2102
#, c-format
msgid "Client %s:%d connected."
msgstr "Clientul %s:%d s-a conectat."

#: ../backend/src/DistributionSocketLink.cc:2142
#, c-format
msgid "Could not connect to client %s:%d."
msgstr "Nu se poate efectua conexiunea cu clientul %s:%d."

