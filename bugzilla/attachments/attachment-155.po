#, c-format
msgid "Client %s:%d access denied."
msgstr "Acces interzis pentru clientul %s:%d."

#: ../backend/src/DistributionSocketLink.cc:1353
#, c-format
msgid "Client %s:%d signed off."
msgstr "Clientul %s:%d s-a deconectat."

