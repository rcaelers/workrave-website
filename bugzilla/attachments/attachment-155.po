#, c-format
msgid "Rejecting master request from client %s:%d."
msgstr "Se respinge cererea de a deveni client principal clientului %s:%d."

#: ../backend/src/DistributionSocketLink.cc:1723
#, c-format
msgid "Acknowledging master request from client %s:%d."
msgstr "Se acceptă cererea de a deveni client principal clientului %s:%d."

#: ../backend/src/DistributionSocketLink.cc:1774
#, c-format
msgid "Non-master client %s:%d rejected master request."
msgstr "S-a respins cererea de a deveni client principal clientului %s:%d."

#: ../backend/src/DistributionSocketLink.cc:1779
#, c-format
msgid "Client %s:%d rejected master request, delaying."
msgstr "Clientul %s:%d a respins cererea de client principal, se amână."

